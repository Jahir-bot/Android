        </activity>
        <activity
            android:name=".CalcularFactorialActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".DeterminarCapicuaActivity"
